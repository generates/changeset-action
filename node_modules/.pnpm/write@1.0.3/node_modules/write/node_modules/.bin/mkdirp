#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/bin/cmd.js/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/bin/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/mkdirp@0.5.5/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules:/Users/ianwalter/ianwalter/node_modules:/Users/ianwalter/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mkdirp@0.5.5/node_modules/mkdirp/bin/cmd.js" "$@"
else 
  exec node  "$basedir/../../../../../mkdirp@0.5.5/node_modules/mkdirp/bin/cmd.js" "$@"
fi
