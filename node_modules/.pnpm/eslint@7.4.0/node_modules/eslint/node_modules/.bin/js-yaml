#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/js-yaml@3.14.0/node_modules/js-yaml/bin/js-yaml.js/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/js-yaml@3.14.0/node_modules/js-yaml/bin/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/js-yaml@3.14.0/node_modules/js-yaml/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/js-yaml@3.14.0/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules:/Users/ianwalter/ianwalter/node_modules:/Users/ianwalter/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../js-yaml@3.14.0/node_modules/js-yaml/bin/js-yaml.js" "$@"
else 
  exec node  "$basedir/../../../../../js-yaml@3.14.0/node_modules/js-yaml/bin/js-yaml.js" "$@"
fi
