#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/marked@1.1.0/node_modules/marked/bin/marked/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/marked@1.1.0/node_modules/marked/bin/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/marked@1.1.0/node_modules/marked/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/marked@1.1.0/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules:/Users/ianwalter/ianwalter/node_modules:/Users/ianwalter/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../marked@1.1.0/node_modules/marked/bin/marked" "$@"
else 
  exec node  "$basedir/../../../../../../../marked@1.1.0/node_modules/marked/bin/marked" "$@"
fi
