#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/smartwrap@1.2.5/node_modules/smartwrap/src/terminal-adapter.js/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/smartwrap@1.2.5/node_modules/smartwrap/src/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/smartwrap@1.2.5/node_modules/smartwrap/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/smartwrap@1.2.5/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules:/Users/ianwalter/ianwalter/node_modules:/Users/ianwalter/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../smartwrap@1.2.5/node_modules/smartwrap/src/terminal-adapter.js" "$@"
else 
  exec node  "$basedir/../../../../../smartwrap@1.2.5/node_modules/smartwrap/src/terminal-adapter.js" "$@"
fi
