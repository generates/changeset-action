#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/he@1.2.0/node_modules/he/bin/he/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/he@1.2.0/node_modules/he/bin/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/he@1.2.0/node_modules/he/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/he@1.2.0/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules:/Users/ianwalter/ianwalter/node_modules:/Users/ianwalter/node_modules:/Users/node_modules:/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules/he/bin/he/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules/he/bin/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules/he/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../he/bin/he" "$@"
else 
  exec node  "$basedir/../he/bin/he" "$@"
fi
