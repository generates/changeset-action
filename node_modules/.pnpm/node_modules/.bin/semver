#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/semver/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/semver@5.7.1/node_modules/semver/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/semver@5.7.1/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules:/Users/ianwalter/ianwalter/node_modules:/Users/ianwalter/node_modules:/Users/node_modules:/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules/semver/bin/semver/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules/semver/bin/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules/semver/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../semver/bin/semver" "$@"
else 
  exec node  "$basedir/../semver/bin/semver" "$@"
fi
