#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/tty-table@2.8.13/node_modules/tty-table/adapters/terminal-adapter.js/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/tty-table@2.8.13/node_modules/tty-table/adapters/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/tty-table@2.8.13/node_modules/tty-table/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/tty-table@2.8.13/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules:/Users/ianwalter/ianwalter/node_modules:/Users/ianwalter/node_modules:/Users/node_modules:/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules/tty-table/adapters/terminal-adapter.js/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules/tty-table/adapters/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules/tty-table/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tty-table/adapters/terminal-adapter.js" "$@"
else 
  exec node  "$basedir/../tty-table/adapters/terminal-adapter.js" "$@"
fi
