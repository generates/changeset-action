#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/esvalidate.js/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/esprima@4.0.1/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules:/Users/ianwalter/ianwalter/node_modules:/Users/ianwalter/node_modules:/Users/node_modules:/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules/esprima/bin/esvalidate.js/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules/esprima/bin/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules/esprima/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esprima/bin/esvalidate.js" "$@"
else 
  exec node  "$basedir/../esprima/bin/esvalidate.js" "$@"
fi
