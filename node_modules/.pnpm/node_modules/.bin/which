#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/which@1.3.1/node_modules/which/bin/which/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/which@1.3.1/node_modules/which/bin/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/which@1.3.1/node_modules/which/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/which@1.3.1/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules:/Users/ianwalter/ianwalter/node_modules:/Users/ianwalter/node_modules:/Users/node_modules:/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules/which/bin/which/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules/which/bin/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules/which/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../which/bin/which" "$@"
else 
  exec node  "$basedir/../which/bin/which" "$@"
fi
