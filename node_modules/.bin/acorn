#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/acorn@7.3.1/node_modules/acorn/bin/acorn/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/acorn@7.3.1/node_modules/acorn/bin/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/acorn@7.3.1/node_modules/acorn/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/acorn@7.3.1/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules:/Users/ianwalter/ianwalter/node_modules:/Users/ianwalter/node_modules:/Users/node_modules:/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/acorn/bin/acorn/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/acorn/bin/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/acorn/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
else 
  exec node  "$basedir/../acorn/bin/acorn" "$@"
fi
