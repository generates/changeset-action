#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/@ianwalter/monorepo-app@1.0.0_@ianwalter+monorepo-app@1.0.0/node_modules/@ianwalter/monorepo-app/cli.js/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/@ianwalter/monorepo-app@1.0.0_@ianwalter+monorepo-app@1.0.0/node_modules/@ianwalter/monorepo-app/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/@ianwalter/monorepo-app@1.0.0_@ianwalter+monorepo-app@1.0.0/node_modules/@ianwalter/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/@ianwalter/monorepo-app@1.0.0_@ianwalter+monorepo-app@1.0.0/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/@ianwalter/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules:/Users/ianwalter/ianwalter/node_modules:/Users/ianwalter/node_modules:/Users/node_modules:/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/@ianwalter/monorepo-app/cli.js/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/@ianwalter/monorepo-app/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/@ianwalter/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@ianwalter/monorepo-app/cli.js" "$@"
else 
  exec node  "$basedir/../@ianwalter/monorepo-app/cli.js" "$@"
fi
