#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/eslint@7.4.0/node_modules/eslint/bin/eslint.js/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/eslint@7.4.0/node_modules/eslint/bin/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/eslint@7.4.0/node_modules/eslint/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/eslint@7.4.0/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/.pnpm/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules:/Users/ianwalter/ianwalter/node_modules:/Users/ianwalter/node_modules:/Users/node_modules:/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/eslint/bin/eslint.js/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/eslint/bin/node_modules:/Users/ianwalter/ianwalter/add-changeset/node_modules/eslint/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../eslint/bin/eslint.js" "$@"
else 
  exec node  "$basedir/../eslint/bin/eslint.js" "$@"
fi
