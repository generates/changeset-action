#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/@changesets/cli@2.9.2/node_modules/@changesets/cli/bin.js/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/@changesets/cli@2.9.2/node_modules/@changesets/cli/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/@changesets/cli@2.9.2/node_modules/@changesets/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/@changesets/cli@2.9.2/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/@changesets/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/.pnpm/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules:/Users/ianwalter/ianwalter/node_modules:/Users/ianwalter/node_modules:/Users/node_modules:/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/@changesets/cli/bin.js/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/@changesets/cli/node_modules:/Users/ianwalter/ianwalter/add-changeset-action/node_modules/@changesets/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@changesets/cli/bin.js" "$@"
else 
  exec node  "$basedir/../@changesets/cli/bin.js" "$@"
fi
